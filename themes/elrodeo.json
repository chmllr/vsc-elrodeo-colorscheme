			"settings": {
				"background": "#e05252",
				"fontStyle": "",
				"foreground": "#d0d5d5"
			}
		},
		{
			"name": "diff: changed",
			"scope": "markup.changed",
			"settings": {
				"background": "#8fbadb",
				"fontStyle": "",
				"foreground": "#d0d5d5"
			}
		},
		{
			"name": "diff: inserted",
			"scope": "markup.inserted",
			"settings": {
				"background": "#2d9885",
				"foreground": "#d0d5d5"
			}
		},
