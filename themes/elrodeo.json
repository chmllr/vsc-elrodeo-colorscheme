{
	"name": "Elrodeo",
	"tokenColors": [
		{
			"settings": {
				"background": "#232a2a",
				"foreground": "#2d9885"
			}
		},
		{
			"scope": ["meta.embedded", "source.groovy.embedded"],
			"settings": {
				"background": "#232a2a",
				"foreground": "#2d9885"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "#636d83"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"foreground": "#8fbadb"
			}
		},
		{
			"name": "Regexp",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#96bdb4"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#2d9885"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable.language",
				"variable.other"
